@model DataManager
@{
    ViewData["PageName"] = "Contact";
    ViewData["Title"] = "Contact";
}
@await Html.PartialAsync("~/Views/Shared/Breadcrumb.cshtml")
<!-- Contact Section Begin -->
<div class="map">
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2540.651839447461!2d30.51983701573132!3d50.44758537947498!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x40d4ce56b2456d3b%3A0xd062ae171b57e947!2z0YPQuy4g0JrRgNC10YnQsNGC0LjQuiwg0JrQuNC10LIsIDAyMDAw!5e0!3m2!1sru!2sua!4v1610967452286!5m2!1sru!2sua" width="600" height="672" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
</div>
<section class="contact-section spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-6">
                <div class="contact-text">
                    <h3>Get in touch</h3>
                    <p>
                        Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut
                        labore et dolore magna aliqua.
                    </p>
                    <div class="ct-item">
                        <div class="ct-icon">
                            <i class="fa fa-map-marker"></i>
                        </div>
                        <div class="ct-text">
                            <h5>Address</h5>
                            <p>@ContactInformation.Address</p>
                        </div>
                    </div>
                    <div class="ct-item">
                        <div class="ct-icon">
                            <i class="fa fa-mobile"></i>
                        </div>
                        <div class="ct-text">
                            <h5>Phone</h5>
                            <ul>
                                <li>@ContactInformation.Phone</li>
                                <li>@ContactInformation.Phone1</li>
                            </ul>
                        </div>
                    </div>
                    <div class="ct-item">
                        <div class="ct-icon">
                            <i class="fa fa-envelope"></i>
                        </div>
                        <div class="ct-text">
                            <h5>Email</h5>
                            <p>@ContactInformation.Email</p>
                        </div>
                    </div>
                    <div class="ct-item">
                        <div class="ct-icon">
                            <i class="far fa-clock"></i>
                        </div>
                        <div class="ct-text">
                            <h5>Schedule</h5>
                            <ul>
                                <li>@ContactInformation.WorkTime</li>
                                <li>@ContactInformation.Holidays</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-6">
                <div class="contact-form">
                    <h3>Work with Me!</h3>
                    <form asp-action="FeedBack" asp-controller="Contact" method="post">
                        <input type="text" name="Name" placeholder="Name">
                        <input type="text" name="Email" placeholder="Email">
                        <input type="text" name="Website" placeholder="Website">
                        <textarea name="Message" placeholder="Message"></textarea>
                        <button type="submit" class="site-btn">Submit</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Contact Section End -->